{
    "dev": {
        "app_function": "scraper.app",
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "project_name": "zappa-scrapping",
        "runtime": "python3.10",
        "s3_bucket": "parcial3-bigdata",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "LabRole",
        "events": [
            {
                "function": "scraper.app",
                "type": "schedule",
                "expression": "rate(1 minute)" //rate(1 minute) cron(0 12 * * ? *)
            }
        ]
    }
}